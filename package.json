{
  "name": "BetterTweetDeck",
  "version": "3.6.4",
  "extension_version": "3.6.4",
  "description": "Adds some nice options on TweetDeck to provide a better experience on the webapp when used on Chrome.",
  "repository": {
    "type": "git",
    "url": "https://github.com/eramdam/BetterTweetDeck.git"
  },
  "author": "Damien Erambert",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/eramdam/BetterTweetDeck/issues"
  },
  "scripts": {
    "prerelease": "rimraf artifacts/*",
    "start": "cross-env NODE_ENV=dev webpack --hide-modules --watch --env.browser",
    "serve": "cross-env NODE_ENV=dev node tools/webserver.js",
    "build": "cross-env NODE_ENV=dev webpack --hide-modules --env.browser",
    "build:prod": "cross-env NODE_ENV=prod webpack --hide-modules --env.browser",
    "run:firefox": "web-ext run -s dist",
    "lint": "eslint {src,tools,config}/**/*.js webpack.config.js && stylelint src/**/*.css",
    "test": "npm run lint",
    "release": "npm run build:prod -- chrome && npm run pack:chrome && npm run build:prod -- firefox && npm run pack:firefox",
    "pack:chrome": "cross-env NODE_ENV=dev node tools/pack.js",
    "pack:firefox": "cross-env NODE_ENV=dev web-ext build -s dist -a artifacts",
    "update_emojis": "node tools/update_emojis.js",
    "prepush": "npm run lint"
  },
  "homepage": "https://github.com/eramdam/BetterTweetDeck",
  "dependencies": {
    "apollo-fetch": "^0.7.0",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-lodash": "^3.2.11",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.10",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "chalk": "1.1.3",
    "clean-webpack-plugin": "^0.1.17",
    "clipboard": "^1.7.1",
    "config": "^1.29.4",
    "copy-webpack-plugin": "^4.4.0",
    "cross-env": "^5.1.3",
    "crx": "3.2.1",
    "css-loader": "^0.28.9",
    "cssnano": "3.10.0",
    "del": "3.0.0",
    "domify": "1.4.0",
    "dompurify": "^1.0.2",
    "emoji-js": "3.4.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "fecha": ">=1.0.0",
    "file-saver": "1.3.3",
    "generate-json-webpack-plugin": "^0.2.2",
    "gifshot": "git://github.com/yahoo/gifshot.git#0.4.4",
    "html-webpack-plugin": "^2.30.1",
    "jquery": "3.2.1",
    "lodash": "4.17.4",
    "lodash-webpack-plugin": "^0.11.4",
    "marked": "0.3.6",
    "mobx": "^3.3.1",
    "mobx-react": "^4.3.4",
    "moduleraid": "^3.1.0",
    "mustache": "2.3.0",
    "needle": "^2.1.1",
    "normalize.css": "7.0.0",
    "postcss-cssnext": "^3.1.0",
    "postcss-import": "^11.0.0",
    "postcss-loader": "^2.1.0",
    "postcss-nested": "^3.0.0",
    "postcss-url": "^7.3.0",
    "prismjs": "^1.11.0",
    "promise-each": "2.2.0",
    "prop-types": "^15.6.0",
    "query-string": "4.3.4",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-helmet": "^5.2.0",
    "reuse-promise": "1.1.0",
    "rimraf": "^2.6.2",
    "style-loader": "^0.20.1",
    "uglifyjs-webpack-plugin": "^1.1.8",
    "web-ext": "^2.4.0",
    "webpack": "^3.8.1",
    "webpack-bundle-analyzer": "^2.9.0",
    "webpack-dev-server": "^2.9.4",
